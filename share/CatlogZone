#
# Configuration of catz
#


#
# for make from zonefile
#
setting("catlog.example.jp") do |s|
	s.software="nsd"
	s.source="file"
	s.zonename "catlog.example.jp"
	s.zonefile "/etc/nsd/catlog.example.jp.zone"
end

