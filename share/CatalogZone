#
# Configuration of catz
#


#
# for make from zonefile
#
setting("catalog.example.jp") do |s|
	s.software="nsd"
	s.source="file"
	s.zonename="catalog.example.jp"
	s.zonefile="/etc/nsd/catalog.example.jp.zone"
end

